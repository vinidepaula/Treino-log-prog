algoritmo "semnome"


var
nome:caracter

n1,n2,n3,media:real

inicio
escreva("nome")
leia (nome)

escreval(" digite a primeira nota")
 leia (n1)

escreval("digite segunda nota")
leia (n2)

escreval("digite terceira nota")
leia(n3)
media<- (n1+n2+n3)/3
escreval("media: ",media)
se(media>=7)entao
faca
escreva ("APROVADO")
senao
escreva ("REPROVADO")
fimse

fimalgoritmo